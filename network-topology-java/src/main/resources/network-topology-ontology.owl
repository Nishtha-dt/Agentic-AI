<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://example.org/network-topology#"
         xml:base="http://example.org/network-topology"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:dc="http://purl.org/dc/elements/1.1/"
         xmlns:foaf="http://xmlns.com/foaf/0.1/">

    <!-- Ontology Declaration -->
    <owl:Ontology rdf:about="http://example.org/network-topology">
        <dc:title>Network Topology Ontology</dc:title>
        <dc:description>An OWL/RDFS ontology for modeling network topology including devices, interfaces, links, sites, zones, and operational attributes</dc:description>
        <dc:creator>AI Assistant</dc:creator>
        <dc:date>2025-08-07</dc:date>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>

    <!-- =======================
         TOP-LEVEL CLASSES
         ======================= -->

    <!-- Network Entity - Top level class -->
    <owl:Class rdf:about="#NetworkEntity">
        <rdfs:label>Network Entity</rdfs:label>
        <rdfs:comment>Abstract class representing any entity in a network topology</rdfs:comment>
    </owl:Class>

    <!-- =======================
         DEVICE CLASSES
         ======================= -->

    <!-- Base Device Class -->
    <owl:Class rdf:about="#NetworkDevice">
        <rdfs:label>Network Device</rdfs:label>
        <rdfs:comment>A physical or virtual device in the network</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkEntity"/>
    </owl:Class>

    <!-- Router -->
    <owl:Class rdf:about="#Router">
        <rdfs:label>Router</rdfs:label>
        <rdfs:comment>A network device that forwards data packets between computer networks</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkDevice"/>
    </owl:Class>

    <!-- Switch -->
    <owl:Class rdf:about="#Switch">
        <rdfs:label>Switch</rdfs:label>
        <rdfs:comment>A network device that connects devices within a single network segment</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkDevice"/>
    </owl:Class>

    <!-- Firewall -->
    <owl:Class rdf:about="#Firewall">
        <rdfs:label>Firewall</rdfs:label>
        <rdfs:comment>A network security device that monitors and controls network traffic</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkDevice"/>
    </owl:Class>

    <!-- Access Point -->
    <owl:Class rdf:about="#AccessPoint">
        <rdfs:label>Access Point</rdfs:label>
        <rdfs:comment>A wireless network device that allows wireless devices to connect to a wired network</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkDevice"/>
    </owl:Class>

    <!-- Load Balancer -->
    <owl:Class rdf:about="#LoadBalancer">
        <rdfs:label>Load Balancer</rdfs:label>
        <rdfs:comment>A device that distributes network traffic across multiple servers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkDevice"/>
    </owl:Class>

    <!-- Gateway -->
    <owl:Class rdf:about="#Gateway">
        <rdfs:label>Gateway</rdfs:label>
        <rdfs:comment>A device that connects networks using different protocols</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkDevice"/>
    </owl:Class>

    <!-- =======================
         INTERFACE CLASSES
         ======================= -->

    <!-- Network Interface -->
    <owl:Class rdf:about="#NetworkInterface">
        <rdfs:label>Network Interface</rdfs:label>
        <rdfs:comment>A connection point between a device and a network</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkEntity"/>
    </owl:Class>

    <!-- Ethernet Interface -->
    <owl:Class rdf:about="#EthernetInterface">
        <rdfs:label>Ethernet Interface</rdfs:label>
        <rdfs:comment>A wired network interface using Ethernet protocol</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkInterface"/>
    </owl:Class>

    <!-- Wireless Interface -->
    <owl:Class rdf:about="#WirelessInterface">
        <rdfs:label>Wireless Interface</rdfs:label>
        <rdfs:comment>A wireless network interface</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkInterface"/>
    </owl:Class>

    <!-- Serial Interface -->
    <owl:Class rdf:about="#SerialInterface">
        <rdfs:label>Serial Interface</rdfs:label>
        <rdfs:comment>A serial communication interface</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkInterface"/>
    </owl:Class>

    <!-- Virtual Interface -->
    <owl:Class rdf:about="#VirtualInterface">
        <rdfs:label>Virtual Interface</rdfs:label>
        <rdfs:comment>A software-defined network interface</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkInterface"/>
    </owl:Class>

    <!-- =======================
         LINK CLASSES
         ======================= -->

    <!-- Network Link -->
    <owl:Class rdf:about="#NetworkLink">
        <rdfs:label>Network Link</rdfs:label>
        <rdfs:comment>A connection between two network interfaces</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkEntity"/>
    </owl:Class>

    <!-- Physical Link -->
    <owl:Class rdf:about="#PhysicalLink">
        <rdfs:label>Physical Link</rdfs:label>
        <rdfs:comment>A physical connection between network devices</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkLink"/>
    </owl:Class>

    <!-- Virtual Link -->
    <owl:Class rdf:about="#VirtualLink">
        <rdfs:label>Virtual Link</rdfs:label>
        <rdfs:comment>A logical connection between network devices</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkLink"/>
    </owl:Class>

    <!-- Wireless Link -->
    <owl:Class rdf:about="#WirelessLink">
        <rdfs:label>Wireless Link</rdfs:label>
        <rdfs:comment>A wireless connection between network devices</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkLink"/>
    </owl:Class>

    <!-- =======================
         LOCATION CLASSES
         ======================= -->

    <!-- Site -->
    <owl:Class rdf:about="#Site">
        <rdfs:label>Site</rdfs:label>
        <rdfs:comment>A physical location containing network infrastructure</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkEntity"/>
    </owl:Class>

    <!-- Data Center -->
    <owl:Class rdf:about="#DataCenter">
        <rdfs:label>Data Center</rdfs:label>
        <rdfs:comment>A facility housing computer systems and network equipment</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Site"/>
    </owl:Class>

    <!-- Branch Office -->
    <owl:Class rdf:about="#BranchOffice">
        <rdfs:label>Branch Office</rdfs:label>
        <rdfs:comment>A remote office location with network infrastructure</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Site"/>
    </owl:Class>

    <!-- Zone -->
    <owl:Class rdf:about="#Zone">
        <rdfs:label>Zone</rdfs:label>
        <rdfs:comment>A logical grouping of network resources with similar security or functional requirements</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkEntity"/>
    </owl:Class>

    <!-- Security Zone -->
    <owl:Class rdf:about="#SecurityZone">
        <rdfs:label>Security Zone</rdfs:label>
        <rdfs:comment>A zone defined by security policies and access controls</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Zone"/>
    </owl:Class>

    <!-- DMZ -->
    <owl:Class rdf:about="#DMZ">
        <rdfs:label>DMZ</rdfs:label>
        <rdfs:comment>Demilitarized Zone - a physical or logical subnetwork that contains external-facing services</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#SecurityZone"/>
    </owl:Class>

    <!-- VLAN -->
    <owl:Class rdf:about="#VLAN">
        <rdfs:label>VLAN</rdfs:label>
        <rdfs:comment>Virtual Local Area Network - a logical grouping of devices</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Zone"/>
    </owl:Class>

    <!-- =======================
         STATUS ENUMERATION
         ======================= -->

    <!-- Operational Status -->
    <owl:Class rdf:about="#OperationalStatus">
        <rdfs:label>Operational Status</rdfs:label>
        <rdfs:comment>The operational state of a network entity</rdfs:comment>
    </owl:Class>

    <owl:NamedIndividual rdf:about="#Active">
        <rdf:type rdf:resource="#OperationalStatus"/>
        <rdfs:label>Active</rdfs:label>
        <rdfs:comment>The entity is operational and functioning normally</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Inactive">
        <rdf:type rdf:resource="#OperationalStatus"/>
        <rdfs:label>Inactive</rdfs:label>
        <rdfs:comment>The entity is not operational</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Maintenance">
        <rdf:type rdf:resource="#OperationalStatus"/>
        <rdfs:label>Maintenance</rdfs:label>
        <rdfs:comment>The entity is under maintenance</rdfs:comment>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Failed">
        <rdf:type rdf:resource="#OperationalStatus"/>
        <rdfs:label>Failed</rdfs:label>
        <rdfs:comment>The entity has failed and is not operational</rdfs:comment>
    </owl:NamedIndividual>

    <!-- =======================
         OBJECT PROPERTIES
         ======================= -->

    <!-- Device Relationships -->
    <owl:ObjectProperty rdf:about="#hasInterface">
        <rdfs:label>has interface</rdfs:label>
        <rdfs:comment>Relates a device to its network interfaces</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="#NetworkInterface"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#interfaceOf">
        <rdfs:label>interface of</rdfs:label>
        <rdfs:comment>Relates an interface to its parent device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="#NetworkDevice"/>
        <owl:inverseOf rdf:resource="#hasInterface"/>
    </owl:ObjectProperty>

    <!-- Link Relationships -->
    <owl:ObjectProperty rdf:about="#connectsInterface">
        <rdfs:label>connects interface</rdfs:label>
        <rdfs:comment>Relates a link to the interfaces it connects</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkLink"/>
        <rdfs:range rdf:resource="#NetworkInterface"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#connectedVia">
        <rdfs:label>connected via</rdfs:label>
        <rdfs:comment>Relates an interface to the link it uses</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="#NetworkLink"/>
        <owl:inverseOf rdf:resource="#connectsInterface"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#connectedTo">
        <rdfs:label>connected to</rdfs:label>
        <rdfs:comment>Direct connection between two devices</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="#NetworkDevice"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>

    <!-- Location Relationships -->
    <owl:ObjectProperty rdf:about="#locatedAt">
        <rdfs:label>located at</rdfs:label>
        <rdfs:comment>Relates a network entity to its physical location</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkEntity"/>
        <rdfs:range rdf:resource="#Site"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#contains">
        <rdfs:label>contains</rdfs:label>
        <rdfs:comment>Relates a site to the network entities it contains</rdfs:comment>
        <rdfs:domain rdf:resource="#Site"/>
        <rdfs:range rdf:resource="#NetworkEntity"/>
        <owl:inverseOf rdf:resource="#locatedAt"/>
    </owl:ObjectProperty>

    <!-- Zone Relationships -->
    <owl:ObjectProperty rdf:about="#belongsToZone">
        <rdfs:label>belongs to zone</rdfs:label>
        <rdfs:comment>Relates a network entity to its zone</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkEntity"/>
        <rdfs:range rdf:resource="#Zone"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#includesEntity">
        <rdfs:label>includes entity</rdfs:label>
        <rdfs:comment>Relates a zone to the entities it includes</rdfs:comment>
        <rdfs:domain rdf:resource="#Zone"/>
        <rdfs:range rdf:resource="#NetworkEntity"/>
        <owl:inverseOf rdf:resource="#belongsToZone"/>
    </owl:ObjectProperty>

    <!-- Status Relationship -->
    <owl:ObjectProperty rdf:about="#hasStatus">
        <rdfs:label>has status</rdfs:label>
        <rdfs:comment>Relates an entity to its operational status</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkEntity"/>
        <rdfs:range rdf:resource="#OperationalStatus"/>
    </owl:ObjectProperty>

    <!-- =======================
         DATATYPE PROPERTIES
         ======================= -->

    <!-- Identification Properties -->
    <owl:DatatypeProperty rdf:about="#deviceId">
        <rdfs:label>device ID</rdfs:label>
        <rdfs:comment>Unique identifier for a network device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#hostname">
        <rdfs:label>hostname</rdfs:label>
        <rdfs:comment>Network hostname of the device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#ipAddress">
        <rdfs:label>IP address</rdfs:label>
        <rdfs:comment>IP address assigned to an interface</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#macAddress">
        <rdfs:label>MAC address</rdfs:label>
        <rdfs:comment>Media Access Control address of an interface</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Hardware Properties -->
    <owl:DatatypeProperty rdf:about="#manufacturer">
        <rdfs:label>manufacturer</rdfs:label>
        <rdfs:comment>Manufacturer of the network device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#model">
        <rdfs:label>model</rdfs:label>
        <rdfs:comment>Model number or name of the device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#serialNumber">
        <rdfs:label>serial number</rdfs:label>
        <rdfs:comment>Serial number of the device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Software Properties -->
    <owl:DatatypeProperty rdf:about="#firmwareVersion">
        <rdfs:label>firmware version</rdfs:label>
        <rdfs:comment>Version of the firmware running on the device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#osVersion">
        <rdfs:label>OS version</rdfs:label>
        <rdfs:comment>Operating system version running on the device</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Operational Properties -->
    <owl:DatatypeProperty rdf:about="#uptime">
        <rdfs:label>uptime</rdfs:label>
        <rdfs:comment>Time since the device was last restarted (in seconds)</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#lastSeen">
        <rdfs:label>last seen</rdfs:label>
        <rdfs:comment>Timestamp when the device was last detected</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkDevice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <!-- Interface Properties -->
    <owl:DatatypeProperty rdf:about="#interfaceName">
        <rdfs:label>interface name</rdfs:label>
        <rdfs:comment>Name of the network interface</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#speed">
        <rdfs:label>speed</rdfs:label>
        <rdfs:comment>Speed of the interface in Mbps</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#mtu">
        <rdfs:label>MTU</rdfs:label>
        <rdfs:comment>Maximum Transmission Unit size in bytes</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkInterface"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>

    <!-- Link Properties -->
    <owl:DatatypeProperty rdf:about="#bandwidth">
        <rdfs:label>bandwidth</rdfs:label>
        <rdfs:comment>Bandwidth capacity of the link in Mbps</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkLink"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#latency">
        <rdfs:label>latency</rdfs:label>
        <rdfs:comment>Network latency in milliseconds</rdfs:comment>
        <rdfs:domain rdf:resource="#NetworkLink"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <!-- Location Properties -->
    <owl:DatatypeProperty rdf:about="#siteName">
        <rdfs:label>site name</rdfs:label>
        <rdfs:comment>Name of the site</rdfs:comment>
        <rdfs:domain rdf:resource="#Site"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#address">
        <rdfs:label>address</rdfs:label>
        <rdfs:comment>Physical address of the site</rdfs:comment>
        <rdfs:domain rdf:resource="#Site"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#coordinates">
        <rdfs:label>coordinates</rdfs:label>
        <rdfs:comment>GPS coordinates of the site</rdfs:comment>
        <rdfs:domain rdf:resource="#Site"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Zone Properties -->
    <owl:DatatypeProperty rdf:about="#zoneName">
        <rdfs:label>zone name</rdfs:label>
        <rdfs:comment>Name of the zone</rdfs:comment>
        <rdfs:domain rdf:resource="#Zone"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#vlanId">
        <rdfs:label>VLAN ID</rdfs:label>
        <rdfs:comment>VLAN identifier</rdfs:comment>
        <rdfs:domain rdf:resource="#VLAN"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>

    <!-- Security Properties -->
    <owl:DatatypeProperty rdf:about="#securityLevel">
        <rdfs:label>security level</rdfs:label>
        <rdfs:comment>Security classification level</rdfs:comment>
        <rdfs:domain rdf:resource="#SecurityZone"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- =======================
         RESTRICTIONS AND AXIOMS
         ======================= -->

    <!-- A device must have at least one interface -->
    <owl:Class rdf:about="#NetworkDevice">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasInterface"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- A link connects exactly two interfaces -->
    <owl:Class rdf:about="#NetworkLink">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#connectsInterface"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Each interface belongs to exactly one device -->
    <owl:Class rdf:about="#NetworkInterface">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#interfaceOf"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Each entity has exactly one status -->
    <owl:Class rdf:about="#NetworkEntity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStatus"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

</rdf:RDF>
